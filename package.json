{
  "name": "learning",
  "version": "1.0.0",
  "description": "this is a test of the emergency js system",
  "private": true,
  "scripts": {
    "dist-js": "set NODE_ENV=production && browserify src/js/learning.js | uglifyjs -c -m > dist/learning.user.js",
    "dist-html": "cp src/html/index.html dist/",
    "dist": "npm run dist-js && npm run dist-html",
    "build-js": "set NODE_ENV=development && browserify src/js/learning.js > build/learning.user.js",
    "build-html": "cp src/html/index.html build/",
    "build": "npm run build-js && npm run build-html",
    "watch": "set NODE_ENV=development && watchify -v -d src/js/learning.js -o build/learning.user.js",
    "setup": "mkdir build && mkdir dist",
    "clean": "rm -rf build && rm -rf dist",
    "reset": "npm run clean && npm run setup",
    "everything": "npm run reset && npm run build && npm run dist",
    "start": "start build/index.html"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "aliasify": "^2.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "uglify-js": "^2.7.3",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "vue": "^2.0.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "sourceMaps": true
        }
      ],
      [
        "aliasify"
      ]
    ]
  },
  "aliasify": {
    "aliases": {
      "vue": "vue/dist/vue.js",
      "jquery": "jquery/dist/jquery.js"
    }
  }
}
